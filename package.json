{
  "name": "vue-component-icon",
  "version": "1.0.4",
  "description": "Create and use your own icons, or use only your favorites in your vue.js project.",
  "private": false,
  "author": "@andrejsharapov",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 8081",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@mdi/js": "^7.0.96",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "terser": "^5.14.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.2",
    "vite-plugin-html": "^3.2.0",
    "vue": "^2.7.7",
    "vue-tsc": "^1.0.9"
  },
  "main": "index.js",
  "keywords": [
    "vue",
    "vue3",
    "svg",
    "icons",
    "template",
    "svg-icon"
  ],
  "repository": "git+https://github.com/andrejsharapov/vue-component-icon.git",
  "bugs": {
    "url": "https://github.com/andrejsharapov/vue-component-icon/issues"
  },
  "homepage": "https://github.com/andrejsharapov/vue-component-icon#readme",
  "license": "MIT"
}
