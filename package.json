{
  "name": "vue-component-icon",
  "version": "1.0.3",
  "description": "Create and use your own icons, or use only your favorites in your vue.js project.",
  "private": false,
  "author": "@andrejsharapov",
  "scripts": {
    "serve": "vue-cli-service serve",
    "prettier": "prettier --write ./**/*.{js,vue} --ignore-path .gitignore",
    "prettier:check": "yarn prettier --list-different",
    "prettier:format": "yarn prettier --write --loglevel warn",
    "lint": "vue-cli-service lint --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@mdi/js": "^7.0.96",
    "@vue/cli-plugin-babel": "^4.5.17",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.23.5",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "vue": "^2.7.7",
    "vue-template-compiler": "^2.7.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrejsharapov/vue-component-icon.git"
  },
  "bugs": {
    "url": "https://github.com/andrejsharapov/vue-component-icon/issues"
  },
  "homepage": "https://github.com/andrejsharapov/vue-component-icon#readme",
  "keywords": [
    "vue",
    "vue3",
    "svg",
    "icons",
    "template",
    "svg-icon"
  ],
  "license": "MIT"
}
